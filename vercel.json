{
    "version": 2,
    "routes": [
        { 
            "src": "/api/resume/(.*)", 
            "dest": "/api/resume/$1"
        },
        { 
            "src": "/api/job/(.*)", 
            "dest": "/api/job/$1"
        },
        { 
            "src": "/(.*)", 
            "dest": "/index.js" 
        }
    ]
}